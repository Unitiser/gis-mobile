<document>
    <!-- Title and description of the indicator -->
    <name>sample</name>
    <description>This is something</description>

    <!-- Definition of the parameters, you can have as many parameter as you want. Ex: men, wommen, total -->
    <param name="men" type="Integer">Hommes</param>
    <param name="women" type="Integer">Femmes</param>
    <param name="total" type="Integer">Total</param>

    <!-- List of values for the parameter -->
    <value z="z01" men="10" women="20" total="30"></value>
    <value z="z02" men="50" women="60" total="110"></value>
    <!-- <value z="z03" men="102" women="204" total="306"></value> -->
    
    <!-- List of markers -->
    <marker srcName="EPSG:32188">
        <item label="Sample 1">45.83167,-72.46033</item>
        <item label="Sample 2">45.79434,-72.38068</item>
        <item label="Sample 3">45.99601,-72.56332</item>
        <item label="Sample 4">46.14844,-72.69104</item>
        <item label="Sample 5">46.11418,-72.78305</item>
        <item label="Sample 6">46.07514,-72.71439</item>
        <item label="Sample 7">46.04083,-72.70615</item>
    </marker>

    <!-- Color scheme to use in the map/graphs for a given param, must be a numerical value -->
    <legend value="total" for="map">
        <item min="0" max="100" color="green">0 à 100</item>
        <item min="101" max="200" color="yellow">101 à 200</item>
        <item min="201" max="9999" color="red">201 et plus</item>
    </legend>

    <legend values="men,women" for="pieChart" title="Total hommes/femmes">
        <item color="#2196f3">Hommes</item>
        <item color="#e91e63">Femmes</item>
    </legend>

    <legend values="men,women" for="barChart" title="Hommes/femmes par région">
        <item color="#2196f3">Hommes</item>
        <item color="#e91e63">Femmes</item>
    </legend>

    <legend values="men,women" for="totalBarChart" title="Total hommes/femmes par région">
        <item min="0" max="100" color="#00FF00">0 à 100</item>
        <item min="101" max="200" color="#FFFF00">101 à 200</item>
        <item min="201" max="9999" color="#FF0000">201 et plus</item>
    </legend>

    <!-- Version of the document -->
    <version date="20/10/2015">2.0</version>
</document>